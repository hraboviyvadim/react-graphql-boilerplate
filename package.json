{
  "name": "react-graphql-boilerplate",
  "version": "1.0.0",
  "description": "Basic boilerplate for React application with Webpack and Babel",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hraboviyvadim/react-graphql-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "eslint"
  ],
  "author": "Vadym Grabovyi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hraboviyvadim/react-graphql-boilerplate/issues"
  },
  "homepage": "https://github.com/hraboviyvadim/react-graphql-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
