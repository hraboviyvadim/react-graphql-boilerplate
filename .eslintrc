{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "rules": {
    "max-len": [0],
    "no-unused-vars": [1],
    "no-console": [1],
    "no-case-declarations": "off"
  }
}
